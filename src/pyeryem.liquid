{% for p in donnee %}
<li>
  <h2>{{ p.race }}</h2>
  <h5>{{ p.nd }}</h5>

  {% if p.image %}
  <img src="{{ p.image }}" />
  {% endif %}

  <h4>{{ p.capaciteStats }}</h4>
  <h4>{{ p.capaciteBien }}</h4>
  <h4>{{ p.capaciteMal }}</h4>
  <h3>{{ p.possede }}</h3>
</li>
{% endfor %}

<script type="module" src="js/script.js"></script>
